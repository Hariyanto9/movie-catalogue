{
    "env": {
        "browser": true,
        "es2020": true,
        "node": true,
        "jasmine": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "import/no-extraneous-dependencies": 0,
        "no-console": 0,
        "no-underscore-dangle": 0,
        "no-restricted-globals": 0,
        "linebreak-style": 0,
        "eol-last": 0,
        "no-trailing-spaces": 0,
        "padded-blocks": 0,
        "no-multiple-empty-lines":0,
        "max-len": 0,
        "no-return-assign": 0,
        "no-prototype-builtins": 0,
        "no-empty-function": 0
    },
    "plugins": ["jasmine"]
}
